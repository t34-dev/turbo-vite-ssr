{
  "version": 2,
  "buildCommand": "pnpm turbo run build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "pnpm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/((?!assets/).*)",
      "destination": "/server/index.js"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/client/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.js"
    }
  ]
}
