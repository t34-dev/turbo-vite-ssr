{
  "version": 2,
  "buildCommand": "pnpm turbo run build --filter=web...",
  "outputDirectory": "apps/web/dist",
  "installCommand": "pnpm install",
  "framework": "vite",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/client/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/myindex.js"
    }
  ]
}
