{
  "version": 2,
  "rewrites": [{ "source": "/(.*)", "destination": "apps/web/server.js" }],
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "cd apps/web && npm pnpm build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/server.js"
    }
  ],
  "outputDirectory": "apps/web/dist",
  "installCommand": "pnpm install"
}
