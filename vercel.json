{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/server.js",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "cd apps/web && pnpm run build"
      }
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/api/server" }],
  "outputDirectory": "apps/web/dist",
  "installCommand": "pnpm install"
}

